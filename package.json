{
  "name": "einvoice-contentgate",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "10.3.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "http-server dist/",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "preinstall": "npm install -g http-server",
    "postinstall": "ng build --prod",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^7.1.0",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "^7.1.0",
    "@angular/compiler": "^7.1.0",
    "@angular/core": "^7.1.0",
    "@angular/forms": "^7.1.0",
    "@angular/http": "^7.1.0",
    "@angular/platform-browser": "^7.1.0",
    "@angular/platform-browser-dynamic": "^7.1.0",
    "@angular/router": "^7.1.0",
    "@angular/service-worker": "^7.1.0",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-select/ng-select": "^2.13.1",
    "@ngx-loading-bar/core": "^3.0.0",
    "@ngx-loading-bar/http": "^3.0.0",
    "@ngx-loading-bar/http-client": "^3.0.0",
    "@ngx-loading-bar/router": "^3.0.0",
    "@ngx-translate/core": "^10.0.1",
    "ace-builds": "^1.4.2",
    "bootstrap": "^4.1.1",
    "bootstrap-datepicker": "1.8.0",
    "bootstrap-maxlength": "1.6.0",
    "datatables": "^1.10.18",
    "datatables.net-bs4": "^1.10.19",
    "datatables.net-responsive": "^2.2.3",
    "flag-icon-css": "3.2.1",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "jquery.cookie": "^1.4.1",
    "lodash": "^4.17.10",
    "moment": "2.22.2",
    "ngx-bootstrap": "^3.1.2",
    "ngx-cookie-service": "^2.0.2",
    "ngx-mask": "^7.0.1",
    "popper.js": "^1.14.3",
    "rxjs": "^6.3.1",
    "simple-line-icons": "2.4.1",
    "tinymce": "4.9.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.0",
    "@angular/cli": "~7.1.0",
    "@angular/compiler-cli": "^7.1.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@ngx-rocket/scripts": "^3.0.0",
    "@types/jasmine": "^3.3.0",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.108",
    "@types/node": "^10.12.10",
    "codelyzer": "^4.4.4",
    "compass-mixins": "^0.12.10",
    "hads": "^1.7.0",
    "htmlhint": "^0.10.0",
    "https-proxy-agent": "^2.0.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "merge-stream": "^1.0.1",
    "minimatch": "^3.0.4",
    "ngx-select-dropdown": "^0.8.1",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.4.0",
    "run-sequence": "^2.2.1",
    "stylelint": "~9.9.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.4.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~3.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
